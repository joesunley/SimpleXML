<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>SimplXML</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>Sunley.SimpleXML</PackageId>
    <Authors>Joe Sunley</Authors>
    <AssemblyName>Sunley.SimpleXML</AssemblyName>
    <RepositoryUrl>https://github.com/joesunley/SimpleXML</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/joesunley/SimpleXML</PackageProjectUrl>
    <Copyright>joesunley 2022</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
	  <None Include="C:\Dev\SimpleXML\LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
